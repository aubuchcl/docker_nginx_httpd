{
  "version": "1.0",
  "about": {
    "version": "1.0",
    "description": "a base image for nginx and apache"
  },
  "tests": [],
  "services": {
    "loadbalancer": null,
    "vpn": null
  },
  "containers": {
    "nginx": {
      "name": "nginx",
      "image": {
        "name": "nginx",
        "source": {
          "repo": {
            "dir": "/nginx",
            "build_file": "/Dockerfile",
            "url": "https://github.com/aubuchcl/docker_nginx_httpd.git"
          }
        }
      },
      "config": {
        "network": {
          "public": "enable",
          "hostname": "nginx",
          "ports": ["5000:80"]
        },
        "deploy": {
          "instances": 1,
          "constraints": null,
          "shutdown": {
            "graceful_timeout": 30,
            "signals": []
          },
          "restart": {
            "condition": "always",
            "delay": 5,
            "max_attempts": 3,
            "notify": null
          },
          "health_check": null
        },
        "scaling": {
          "min": 1,
          "max": 3
        },
        "runtime": null,
        "resources": null,
        "integrations": null
      },
      "volumes": []
    },
    "httpd": {
      "name": "httpd",
      "image": {
        "name": "httpd",
        "source": {
          "repo": {
            "dir": "/apache",
            "build_file": "/Dockerfile",
            "url": "https://github.com/aubuchcl/docker_nginx_httpd.git"
          }
        }
      },
      "config": {
        "network": {
          "public": "enable",
          "hostname": "httpd",
          "ports": ["3000:80"]
        },
        "deploy": {
          "instances": 1,
          "constraints": null,
          "shutdown": {
            "graceful_timeout": 30,
            "signals": []
          },
          "health_check": null
        },
        "runtime": null,
        "scaling": {
          "min": 1,
          "max": 3
        },
        "resources": null,
        "integrations": null
      },
      "volumes": []
    },
    "annotations": {}
  }
}
